{
  "name": "gsi-receiver",
  "version": "0.1.0",
  "description": "Web Endpoint receiving GSI data",
  "author": {
    "name": "Thomas Kosiewski"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "private": true,
  "files": [
    "build/src"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fortify-Labs/Fortify.git"
  },
  "bugs": {
    "url": "https://github.com/Fortify-Labs/Fortify/issues"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p . && cpx tsconfig.json build/ && cpx package.json build/",
    "dev": "DEBUG=app:* PORT=8080 nodemon",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "start": "cross-env TS_NODE_PROJECT=build/ NODE_OPTIONS=--trace-warnings node -r tsconfig-paths/register build/src/index.js",
    "start:ts": "cross-env DEBUG=app::* NODE_OPTIONS=--trace-warnings ts-node -r tsconfig-paths/register --files src/index.ts",
    "sync-dotenv": "sync-dotenv"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^13.11.1",
    "@types/request": "^2.48.5",
    "cpx": "^1.5.0",
    "gts": "^2.0.2",
    "nodemon": "^2.0.4",
    "sync-dotenv": "^2.6.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.12.0",
    "tsconfig-paths": "^3.9.0"
  }
}
