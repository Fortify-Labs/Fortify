{
  "name": "backend",
  "version": "1.1.0",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -b",
    "dev": "DEBUG=app:* PORT=8080 nodemon",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "start": "cross-env TS_NODE_PROJECT=./ NODE_OPTIONS=--trace-warnings node -r tsconfig-paths/register build/index.js",
    "start:ts": "cross-env DEBUG=app::* NODE_OPTIONS=--trace-warnings ts-node -r tsconfig-paths/register --files src/index.ts",
    "sync-dotenv": "sync-dotenv",
    "types": "graphql-codegen --config codegen.yml",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -f ./ormconfig.ts",
    "validate": "run-s check compile"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fortify-Labs/Fortify.git"
  },
  "author": "Thomas Kosiewski",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Fortify-Labs/Fortify/issues"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.17.0",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/time": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-resolvers": "^1.17.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/debug": "^4.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cpx": "^1.5.0",
    "debug": "^4.1.1",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "opn-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "sync-dotenv": "^2.6.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "apollo-server-express": "^2.16.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^6.0.14",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.12.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "sofa-api": "^0.8.0",
    "swagger-ui-express": "^4.1.4",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.25"
  }
}
