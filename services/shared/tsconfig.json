{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "baseUrl": ".",
    "composite": true,
    "strict": true,
    "moduleResolution": "Node",
    "declaration": true,
    "declarationMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "inlineSources": true,
    "sourceRoot": "/",
    "skipLibCheck": true
  },
  "references": [],
  "include": [
    "src/**/*.ts",
    "src/**/*.json",
    "test/**/*.ts"
  ]
}
