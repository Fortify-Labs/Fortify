{
  "name": "kubernetes",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "build": "npm run test && npm run synth",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next",
    "record": "npx jest --updateSnapshot",
    "sync-dotenv": "sync-dotenv",
    "clean": "rename ./jest.config.js ./jest.config.js.bac && rimraf *.d.ts && rimraf *.js && rimraf src/*.d.ts && rimraf src/*.js && rimraf src/**/*.d.ts && rimraf src/**/*.js && rimraf imports/*.d.ts && rimraf imports/*.js  && rename ./jest.config.js.bac ./jest.config.js"
  },
  "dependencies": {
    "cdk8s": "^1.0.0-beta.9",
    "common-tags": "^1.8.0",
    "constructs": "^3.3.32",
    "dotenv": "^8.2.0",
    "rename-cli": "^6.2.1"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "cdk8s-cli": "^1.0.0-beta.9",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "sync-dotenv": "^2.6.2",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
